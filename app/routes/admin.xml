<?xml version="1.0" encoding="UTF-8"?>
<object type="Embark\Journey\Metadata\Routes\RoutesList">
    <route>
        <methods>
            <item>GET</item>
        </methods>
        <pattern>{prefix}</pattern>
        <name>admin</name>
        <redirect>
            <url>{prefix}/login</url>
            <code>302</code>
        </redirect>
    </route>
    <route>
        <methods>
            <item>GET</item>
        </methods>
        <pattern>{prefix}/blueprints</pattern>
        <name>blueprints</name>
        <view>Embark\Journey\Views\Blueprints</view>
    </route>
    <route>
        <methods>
            <item>GET</item>
            <item>POST</item>
        </methods>
        <pattern>{prefix}/blueprints/schemas</pattern>
        <name>schemas</name>
        <view>Embark\Journey\Views\BlueprintsSchemas</view>
    </route>
    <route>
        <methods>
            <item>GET</item>
            <item>POST</item>
        </methods>
        <pattern>{prefix}/blueprints/schemas/{schema}</pattern>
        <name>schema</name>
        <view>Embark\Journey\Views\BlueprintsSchema</view>
    </route>
</object>
