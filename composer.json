{
    "name": "embark/cms",
    "description": "A new CMS from old Symphony ideas",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "phly/http": "~1.0@dev",
        "pimple/pimple": "~3.0@dev",
        "monolog/monolog": "~1.13@dev",
        "league/commonmark": "~0.7"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Embark\\Journey\\": "lib/",
            "Embark\\CMS\\": "packages/system/lib/",
            "Embark\\CMS\\Actors\\Condition\\": "packages/condition-actors/lib/",
            "Embark\\CMS\\Actors\\Section\\": "packages/section-actors/lib/"
        },
        "classmap": ["symphony/content", "symphony/lib", "symphony/templates", "install"],
        "files": [
            "symphony/lib/include.utilities.php",
            "symphony/defines.php",
            "symphony/lib/include.validators.php"
        ]
    }
}
