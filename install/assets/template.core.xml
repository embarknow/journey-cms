<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <symphony>
    <maximum-upload-size>5242880</maximum-upload-size>
    <administration-path>symphony/</administration-path>
    <pagination-maximum-rows>17</pagination-maximum-rows>
    <lang>en</lang>
    <cookie-prefix>sym-</cookie-prefix>
    <version>%s</version>
    <sitename>%s</sitename>
    <file-write-mode>%s</file-write-mode>
    <directory-write-mode>%s</directory-write-mode>
    <default-datasource-type>SectionsDataSource</default-datasource-type>
    <default-event-type>SectionsEvent</default-event-type>
  </symphony>
  <log>
    <archive>1</archive>
    <maxsize>102400</maxsize>
  </log>
  <region>
    <time-format>%s</time-format>
    <date-format>%s</date-format>
    <timezone>%s</timezone>
  </region>
</configuration>